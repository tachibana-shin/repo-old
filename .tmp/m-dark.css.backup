body {
	background-color: #161616;

}

ul {
    margin: 5px 0 35px;
    list-style: none;
    background: #1c1c1d
}
ul li *:focus {
    /*background-color: #000000;*/
    color: #ffffff !important;
}

.bg-white {
	background: #1c1c1d !important;
	color: #ffffff !important;
}

ul > li > p {
	overflow: auto;
	color: #ffffff !important;
}
ul a, ul button, .text-secondary, label {
    color: #ffffff !important;
}
ul li:after, ul:after, ul:before {
    display: block;
    width: 100%;
    height: 1px;
    background: #161616
}
